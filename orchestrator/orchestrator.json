  "PostIntermediateMasterFailoverProcesses": [ "echo [$(date +'%Y-%m-%d %H:%M:%S,%3N')],orchestrator,INFO,'Recovered from {failureType} on {failureCluster}. Failed: {failedHost}:{failedPort}; Successor: {successorHost}:{successorPort}' >> /var/log/orchestrator/recovery.log" ],
  "PostGracefulTakeoverProcesses": [ "echo [$(date +'%Y-%m-%d %H:%M:%S,%3N')],orchestrator,INFO,'Planned graceful takeover on {failureClusterAlias} complete' >> /var/log/orchestrator/recovery.log" ],
